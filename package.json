{
  "name": "form-making",
  "description": "A designer and generator of form base on Vue.js, make form development simple and efficient.",
  "version": "1.2.11",
  "author": "GavinZhulei edited by Negin Khademian",
  "license": "MIT",
  "keywords": [
    "component",
    "vue",
    "form",
    "element-ui",
    "auto"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/GavinZhuLei/vue-form-making"
  },
  "main": "dist/FormMaking.common.js",
  "scripts": {
    "serve": "vite",
    "build": "vite build",
    "lint": "vite lint",
    "build-bundle": "eslint build --target lib --name FormMaking ./src/index.js"
  },
  "dependencies": {
    "@vue/compat": "3.2.44",
    "axios": "^0.27.2",
    "clipboard": "^2.0.1",
    "element-plus": "^2.2.20",
    "normalize.css": "^8.0.0",
    "qiniu": "^7.2.1",
    "qiniu-js": "^2.5.1",
    "viewerjs": "^1.2.0",
    "vue": "3.2.44",
    "vue-i18n": "^8.27.2",
    "vue-router": "4",
    "vue2-editor": "^2.10.2",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@types/ace": "0.0.42",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/compiler-sfc": "3.2.44",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "sass": "^1.50.0",
    "terser-webpack-plugin": "^1.2.4",
    "vite": "^3.2.3",
    "webpack-cli": "^4.10.0"
  },
  "babel": {
    "presets": [
      "@vue/app",
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry"
        }
      ]
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
